{
  "manifest_version": 3,
  "name": "E-Gov to Birdarcha Auto",
  "version": "2.0",
  "permissions": ["storage", "tabs"],
  "content_scripts": [
    {
      "matches": [
        "https://my.gov.uz/*",
        "https://office.birdarcha.uz/*"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}